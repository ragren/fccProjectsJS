function fearNotLetter(str) {
  var rest = [], answer = [];
  for (var count = 0; str.length > count; count++) {
    var newstr = str.charCodeAt(count);
    rest.push(newstr);
  }
  for (var start = 0, end = 1; rest.length > end; start++, end++) {
    if (rest[start] - rest[end] < 0) {
      for (var limit = -1, extraCount = 1; rest[start] - rest[end] < limit; limit--, extraCount++) {
        answer.push(rest[start] + extraCount);
      }
    }
  }
  if (answer.length > 0) {
    for (var stringCount = 0; answer.length > stringCount; stringCount++) {
      answer[stringCount] = String.fromCharCode(answer[stringCount]);
    }
    answer = answer.join("");
  } else {
    answer = undefined;
  }
  return answer;
}

fearNotLetter("adgjm");
